{
  // 指令嵌入訊息的翻譯
  ip: {
    // 變數：{ip}
    title: '__**伺服器地址**__',
    description: '**{ip}**',
  },
  site: {
    // 變數：{site}
    title: '__**網站**__',
    description: '**連結：{site}**',
  },
  version: {
    // 變數：{version}
    title: '__**版本**__',
    description: '**{version}**',
  },
  offlineEmbed: {
    // 變數：{ip}、{version}、{site}
    title: ':red_circle: 離線',
    description: '', // 要停用此功能，請將描述留空
    footer: '檢查時間',
  },
  players: {
    // 變數：{playeronline}、{playermax}
    title: '__**玩家**__',
    description: '**{playeronline}/{playermax}**',
    // 玩家名稱列表將顯示在描述之後
    // 當 Minecraft 伺服器離線時顯示離線嵌入訊息
  },
  motd: {
    // 變數：{motd}
    title: '__**每日訊息**__',
    description: '**{motd}**',
  },
  onlineEmbed: {
    // 變數：{ip}、{motd}、{version}、{site}
    title: ':green_circle: 在線',
    // 這裡使用上面的玩家列表嵌入標題和描述（第 28 行）

    // 描述放在玩家列表欄位之後
    description_field_one: '__**每日訊息**__\n**{motd}**\n\n__**伺服器地址**__\n**{ip}**',
    description_field_two: '‎ \n__**版本**__\n**{version}**{siteText}',
    siteText: '\n\n__**網站**__\n**連結：{site}**',
    footer: '檢查時間',
  },
  help: {
    // 變數：{prefix}、{botName}
    title: '__**指令列表**__',
    description: '**前綴：`{prefix}`**',
    // 僅 listFormat 變數：{cmdDescription}、{cmdSlashMention}
    listFormat: '{slashCmdMention} **`-` {cmdDescription}**',

    commandInfoFormat: {
      // 變數：{cmdName}
      title: '__**{cmdName} 指令**__',
      // 變數：{cmdSlashMention}、{cmdDescription}、{prefixCmd}、{prefixCmdAlias}
      description: '**斜線指令：{cmdSlashMention}\n描述：`{cmdDescription}`\n前綴指令：`{prefixCmd}`\n前綴別名：{prefixCmdAlias}**',
    },
  },
  info: {
    title: '**機器人資訊與統計**',
    // 變數：{cpuUsage}、{memoryUsage}、{nodeVersion}、{uptimeDays}、{uptimeHours}、{uptimeMinutes}、{uptimeSeconds}、{ping}、{websocket}
    description: '**CPU 使用率：`{cpuUsage}%`\n記憶體使用率：`{memoryUsage}MB`\nNode.js 版本：`{nodeVersion}`\n運行時間：`{uptimeDays}` 天 `{uptimeHours}` 小時 `{uptimeMinutes}` 分鐘 `{uptimeSeconds}` 秒\n延遲：客戶端 `{ping}ms` | Websocket：`{websocket}ms` **',
  },
} 